<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddModuleServlet</servlet-name>
        <servlet-class>cLPackage.AddModuleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddModuleServlet</servlet-name>
        <url-pattern>/AddModuleServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteModuleServlet</servlet-name>
        <servlet-class>cLPackage.DeleteModuleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteModuleServlet</servlet-name>
        <url-pattern>/DeleteModuleServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateModuleServlet</servlet-name>
        <servlet-class>cLPackage.UpdateModuleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateModuleServlet</servlet-name>
        <url-pattern>/UpdateModuleServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddTopicServlet</servlet-name>
        <servlet-class>cLPackage.AddTopicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddTopicServlet</servlet-name>
        <url-pattern>/AddTopicServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteTopicServlet</servlet-name>
        <servlet-class>cLPackage.DeleteTopicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteTopicServlet</servlet-name>
        <url-pattern>/DeleteTopicServlet</url-pattern>
    </servlet-mapping>


    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

</web-app>